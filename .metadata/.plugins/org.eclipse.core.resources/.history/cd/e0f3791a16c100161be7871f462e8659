
public class App {

	public static void main(String[] args) {
		int brojac1 = 1;
		int brojac2 = 30;
		int brojac3 = 50;
		
		while(brojac1 < 101)
		{
			System.out.println("Interacija broj: " + brojac1);
			brojac1 = brojac1 + 1;
		}

		while(brojac2 < 51)
		{
			System.out.println("Interacija broj: " + brojac2);
			brojac1 = brojac2 + 1;
		}

		while(brojac3 > 29)
		{
			System.out.println("Interacija broj: " + brojac3);
			brojac1 = brojac1 - 1;
		}
	}

}
